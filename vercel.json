{
  "version": 2,
  "framework": "nextjs",
  "installCommand": "npm install --registry=https://registry.npmjs.org/",
  "buildCommand": "npm run build",
  "functions": {
    "app/**": {
      "includeFiles": "src/**"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/demo"
    }
  ]
}
